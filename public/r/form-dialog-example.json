	"files": [
		{
			"path": "examples/form-dialog-example.tsx",
			"type": "registry:example",
			"content": "import React from \"react\";\nimport {\n  Card,\n  CardContent,\n  CardDescription,\n  CardHeader,\n  CardTitle,\n} from \"@/components/ui/card\";\nimport { Button } from \"@/components/ui/button\";\nimport {\n  Dialog,\n  DialogClose,\n  DialogContent,\n  DialogDescription,\n  DialogFooter,\n  DialogHeader,\n  DialogTitle,\n  DialogTrigger,\n} from \"@/components/ui/dialog\";\nimport { Input } from \"@/components/ui/input\";\nimport { Label } from \"@/components/ui/label\";\nimport {\n  Select,\n  SelectContent,\n  SelectItem,\n  SelectTrigger,\n  SelectValue,\n} from \"@/components/ui/select\";\nimport { Textarea } from \"@/components/ui/textarea\";\nimport { useCustomComponentStore } from \"@/stores/useCustomComponentStore\";\n\nconst FormDialog: React.FC = ()=> {\n  const {\n    sectionClassName,\n    cardClassName,\n    titleClassName,\n    descriptionClassName,\n    buttonColor,\n    cardTitle,\n    cardDescription,\n    dialogTitle,\n    dialogDescription,\n    formTitle,\n    formDescription,\n    dialogButton,\n  } = useCustomComponentStore();\n  \n  return (\n    <section className={`${sectionClassName} rounded-lg border border-gray-300`}>\n      <Card className={cardClassName}>\n        <CardHeader>\n          <CardTitle className={titleClassName}>{cardTitle}</CardTitle>\n          <CardDescription className={descriptionClassName}>\n            {cardDescription}\n          </CardDescription>\n        </CardHeader>\n        <CardContent>\n          <Dialog>\n            <DialogTrigger asChild>\n              <Button\n                variant=\"default\"\n                className=\"flex justify-center mx-auto\"\n                style={{ backgroundColor: buttonColor }}\n              >\n                {dialogButton}\n              </Button>\n            </DialogTrigger>\n            <DialogContent className=\"sm:max-w-[425px]\">\n              <DialogHeader>\n                <DialogTitle>{dialogTitle}</DialogTitle>\n                <DialogDescription>{dialogDescription}</DialogDescription>\n              </DialogHeader>\n              {/* Form Section */}\n              <div className=\"grid gap-6\">\n                {/* Select Fields */}\n                <div className=\"grid grid-cols-2 gap-4\">\n                  <div className=\"grid gap-2\">\n                    <Label htmlFor=\"select1\">Select Option</Label>\n                    <Select defaultValue=\"billing\">\n                      <SelectTrigger id=\"select1\">\n                        <SelectValue placeholder=\"Select\" />\n                      </SelectTrigger>\n                      <SelectContent>\n                        <SelectItem value=\"team\">Item 1</SelectItem>\n                        <SelectItem value=\"billing\">Item 2</SelectItem>\n                        <SelectItem value=\"account\">Item 3</SelectItem>\n                        <SelectItem value=\"deployments\">Item 4</SelectItem>\n                        <SelectItem value=\"support\">Item 5</SelectItem>\n                      </SelectContent>\n                    </Select>\n                  </div>\n                  {/* Additional Select Field */}\n                  <div className=\"grid gap-2\">\n                    <Label htmlFor=\"select2\">Select Level</Label>\n                    <Select defaultValue=\"2\">\n                      <SelectTrigger id=\"select2\" className=\"line-clamp-1 w-[160px] truncate\">\n                        <SelectValue placeholder=\"Select level\" />\n                      </SelectTrigger>\n                      <SelectContent>\n                        <SelectItem value=\"1\">Level 1</SelectItem>\n                        <SelectItem value=\"2\">Level 2</SelectItem>\n                        <SelectItem value=\"3\">Level 3</SelectItem>\n                        <SelectItem value=\"4\">Level 4</SelectItem>\n                      </SelectContent>\n                    </Select>\n                  </div>\n                </div>\n\n                {/* Text Input */}\n                <div className=\"grid gap-2\">\n                  <Label htmlFor=\"subject\">{formTitle}</Label>\n                  <Input id=\"subject\" placeholder=\"I need help with...\" />\n                </div>\n\n                {/* Textarea for Description */}\n                <div className=\"grid gap-2\">\n                  <Label htmlFor=\"description\">{formDescription}</Label>\n                  <Textarea\n                    id=\"description\"\n                    placeholder=\"Please include all information relevant to your issue.\"\n                  />\n                </div>\n\n              </div>\n\n              {/* Footer with Buttons */}\n              <DialogFooter className=\"flex justify-between mx-auto flex-row gap-3\">\n                <DialogClose asChild>\n                  <Button\n                    type=\"button\"\n                    variant=\"default\"\n                    style={{ backgroundColor: buttonColor }}\n                  >\n                    Cancel\n                  </Button>\n                </DialogClose>\n                <Button\n                  type=\"button\"\n                  variant=\"default\"\n                  style={{ backgroundColor: buttonColor }}\n                >\n                  Submit\n                </Button>\n              </DialogFooter>\n\n            </DialogContent>\n          </Dialog>\n        </CardContent>\n      </Card>\n    </section>\n  );\n};\n\nexport default FormDialog;\n",
			"target": "components/form-dialog-example.tsx"
		}
	]
}
